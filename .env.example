# Telegram Bot Token from @BotFather
BOT_TOKEN=123456789:AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA

# Admin Telegram User IDs (comma-separated)
ADMIN_IDS=123456789,987654321

# MTProto Proxy Secret prefix: 'dd' for fake-tls, '' for simple
PROXY_SECRET_PREFIX=dd

# Default proxy port range (bot will auto-pick from this range if not specified)
PORT_RANGE_START=8443
PORT_RANGE_END=8500

# Path to store proxy data
DATA_FILE=data/proxies.json

# Log level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO